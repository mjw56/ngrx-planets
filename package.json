{
  "name": "planets-ngrx",
  "version": "1.0.0",
  "description": "ngrx demo",
  "main": "index.js",
  "scripts": {
    "build": "npm run typings && npm run webpack --progress --profile --colors --display-error-details --display-cached",
    "serve": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached  --content-base src/",
    "start": "npm run build && npm run serve",
    "typings": "typings install",
    "webpack": "webpack"
  },
  "keywords": [
    "ng2",
    "rx"
  ],
  "author": "Mike Wilcox <mwilcox56@gmail.com> (http://github.com/mjw56)",
  "license": "MIT",
  "dependencies": {
    "@ngrx/store": "^1.3.1",
    "angular2": "^2.0.0-beta.7",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.34.4",
    "es7-reflect-metadata": "^1.5.5",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "es6-promise-loader": "^1.0.1",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.2",
    "typings": "^0.6.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
